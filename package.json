{
  "name": "@form-atoms/list-atom",
  "packageManager": "yarn@4.10.2",
  "version": "0.0.0-semantic-release",
  "author": "Miroslav Petrik <miroslavpetrik@outlook.com>",
  "homepage": "https://github.com/form-atoms/list-atom#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/form-atoms/list-atom.git"
  },
  "bugs": "https://github.com/form-atoms/list-atom/issues",
  "license": "MIT",
  "keywords": [
    "form atoms",
    "form atoms list atom",
    "form field array",
    "jotai",
    "jotai form",
    "jotai list atom",
    "jotai field array",
    "react",
    "react form"
  ],
  "main": "./dist/index.js",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --build ./tsconfig.build.json",
    "test": "vitest",
    "test:cov": "vitest --coverage",
    "storybook": "yarn storybook:styles && storybook dev",
    "storybook:docs": "yarn storybook:styles && storybook dev --docs",
    "storybook:build": "yarn storybook:styles && storybook build --docs",
    "storybook:styles": "cp ./node_modules/@picocss/pico/css/pico.colors.min.css public/",
    "storybook:upgrade": "yarn dlx storybook@latest upgrade",
    "lint": "eslint --ext .tsx,.ts ./src",
    "lint:fix": "yarn run lint --fix",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "upgrade-packages": "yarn upgrade-interactive",
    "upgrade-yarn": "yarn set version stable && yarn install"
  },
  "devDependencies": {
    "@conform-to/zod": "^1.12.0",
    "@emotion/react": "^11.14.0",
    "@eslint/js": "^9.37.0",
    "@picocss/pico": "^2.1.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.6",
    "@semantic-release/npm": "^12.0.2",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@storybook/addon-docs": "^9.1.7",
    "@storybook/addon-links": "^9.1.7",
    "@storybook/react-vite": "^9.1.7",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/lodash.shuffle": "^4.2.9",
    "@types/react": "^19.2.2",
    "@types/react-copy-to-clipboard": "^5",
    "@types/react-dom": "^19.2.2",
    "@types/semantic-release": "^21.1.0",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "9.37.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-storybook": "9.1.10",
    "form-atoms": "^3.3.2",
    "happy-dom": "^19.0.2",
    "jotai": "2.15.0",
    "jotai-devtools": "0.8.0",
    "jotai-effect": "2.1.2",
    "lodash.shuffle": "^4.2.0",
    "prettier": "3.6.2",
    "react": "^19.2.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^19.2.0",
    "remark-gfm": "^4.0.1",
    "semantic-release": "^24.2.9",
    "shiki": "^3.13.0",
    "storybook": "^9.1.7",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.46.0",
    "vite": "^7.1.9",
    "vitest": "^3.2.4",
    "zod": "^3.25.76"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "release": {
    "branches": [
      "main",
      {
        "name": "next",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md"
          ],
          "message": "chore(release): ${nextRelease.version}\n\n${nextRelease.notes}"
        }
      ],
      "@semantic-release/github"
    ]
  },
  "peerDependencies": {
    "form-atoms": "^3",
    "jotai": "^2",
    "jotai-effect": "^2",
    "react": ">=16.8"
  }
}
